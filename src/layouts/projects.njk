---
layout: page.njk
---

{% include 'header.njk' %}

<main class="content">
    <h1>
        {{ title }}
    </h1>

    {{ content | safe }}

    {% for section in projects %}
        <h2>{{ section.title }}</h2>
        <ul>
            {% for link in section.links %}
                <li>
                    <a href="{{ link.url }}">
                        {{ link.title }}
                    </a>
                </li>
            {% endfor %}
        </ul>
    {% endfor %}
</main>

{% include 'footer.njk' %}
